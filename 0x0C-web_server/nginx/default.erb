server {
    listen 80 default_server;
    listen [::]:80 default_server;

    server_name _;

    location / {
        root /var/www/html;
        index index.nginx-debian.html;
    }

    location /redirect_me {
        return 301 https://github.com/NecheHephzibah;
    }
}

